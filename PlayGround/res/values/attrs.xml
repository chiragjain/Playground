<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="ViewPagerIndicator">
        <attr name="vpiIconPageIndicatorStyle" format="reference" />
    </declare-styleable>
    <declare-styleable name="FlipImageView">
        <attr name="isAnimated" format="boolean" />
        <attr name="isFlipped" format="boolean" />
        <attr name="flipDrawable" format="reference" />
        <attr name="flipDuration" format="integer" />
        <attr name="flipInterpolator" format="reference" />
        <attr name="flipRotations">
            <flag name="none" value="0" />
            <flag name="x" value="1" />
            <flag name="y" value="2" />
            <flag name="z" value="4" />
        </attr>
        <attr name="reverseRotation" format="boolean"></attr>
    </declare-styleable>

    <attr name="customAlertDialogStyle" format="reference" />

    <declare-styleable name="CustomAlertDialog">
        <attr name="cadTitlePanelBackground" format="reference|color" />
        <attr name="cadContentPanelBackground" format="reference|color" />
        <attr name="cadButtonPanelBackground" format="reference|color" />
        <attr name="cadTitleStyle" format="reference" />
        <attr name="cadMessageStyle" format="reference" />
        <attr name="cadSubTitleStyle" format="reference" />
        <attr name="cadButtonStylePositive" format="reference" />
        <attr name="cadButtonStyleNegative" format="reference" />
        <attr name="cadButtonPaddingHorizontal" format="dimension" />
        <attr name="cadButtonPaddingVertical" format="dimension" />
        <attr name="cadTitleDivider" format="reference|color" />
        <attr name="cadContentDivider" format="reference|color" />
        <attr name="cadButtonDivider" format="reference|color" />
        <attr name="cadButtonDividerPadding" format="dimension" />
        <attr name="cadContentPadding" format="dimension" />
        <attr name="cadContentMinHeight" format="dimension" />
    </declare-styleable>

    <attr name="mySwitchStyleAttr" format="reference" />

    <declare-styleable name="CancelEditText">
        <attr name="oakCancelDrawable" format="reference" />
    </declare-styleable>
    <declare-styleable name="LineGraphView">
        <attr name="barHeight" format="dimension" />
        <attr name="barPadding" format="dimension" />
        <attr name="labelTextSize" format="dimension" />
        <attr name="valueTextSize" format="dimension" />
        <attr name="isLabelVisible" format="boolean" />
        <attr name="isValueVisible" format="boolean" />
    </declare-styleable>

</resources>